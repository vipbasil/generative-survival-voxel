{
  "name": "noa-examples",
  "version": "0.1.0",
  "description": "Starter examples of game worlds using the noa voxel engine",
  "scripts": {
    "start": "cd src/hello-world && webpack serve",
    "test": "cd src/test && webpack serve",
    "build1": "rm -f docs/test/*.js*; cd src/test && webpack --env prod",
    "build2": "rm -f docs/hello-world/*.js*; cd src/hello-world && webpack --env prod",
    "build": "npm run build1 && npm run build2"
  },
  "keywords": [
    "noa-engine",
    "noa",
    "voxel",
    "game",
    "engine",
    "example"
  ],
  "author": "Andy Hall",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fenomas/noa-examples.git"
  },
  "dependencies": {
    "@babylonjs/core": "^5.0.0-alpha.62",
    "axios": "^1.3.6",
    "base64-img": "^1.0.1",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "fs": "^0.0.1-security",
    "install": "^0.13.0",
    "langchain": "^0.0.102",
    "noa-engine": "^0.32.0",
    "npm": "^9.6.5",
    "openai": "^3.2.1",
    "sharp": "^0.32.0",
    "simplex-noise": "^4.0.1",
    "util": "^0.12.5",
    "utill": "^0.1.3",
    "voxel-crunch": "^0.2.1"
  },
  "devDependencies": {
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  }
}
